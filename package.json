{
  "name": "gentrace-openapi",
  "version": "0.0.3",
  "description": "OpenAPI specification for Gentrace",
  "main": "index.js",
  "repository": "https://github.com/gentrace/gentrace-openapi",
  "author": "Gentrace",
  "license": "MIT",
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "build": "run-s config generate",
    "config:node-spec": "envsubst < specs/node.yaml > specs/node-resolved.yaml",
    "config:browser-spec": "envsubst < specs/browser.yaml > specs/browser-resolved.yaml",
    "config:oapi-spec": "envsubst < openapitools.json.template > openapitools.json",
    "config": "run-p config:*",
    "generate": "run-p generate:*",
    "generate:node-ts": "npx @openapitools/openapi-generator-cli generate --generator-key node",
    "generate:browser-ts": "npx @openapitools/openapi-generator-cli generate --generator-key browser"
  }
}
